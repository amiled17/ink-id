{
    "parameters": {
	// Geometry
	"subvolume_shape": [48, 96, 96], // [z, y, x]
	"surface_cushion": 10,

	// Network architecture
	"learning_rate": 0.001,
	"drop_rate": 0.5,
	"batch_norm_momentum": 0.9,
	"fbeta_weight": 0.3,
	"filter_size": [3, 3, 3],
	"filters": [16, 8, 4, 2],

	// Run configuration
	"train_batch_size": 30,
	// "train_max_batches": 1000000,
	"train_epochs": 2, // TODO use this
	
	"predict_batch_size": 400,
	"predict_every_n_steps": 10000,
	"predict_overlap_step": 4, // predict on one sample for each _ by _ voxel square
	
	"evaluate_batch_size": 400,
	"evaluate_every_n_steps": 1000,

	"summary_every_n_steps": 100,

	// Data organization/augmentation
	"use_jitter": true, // TODO check using this
	"jitter_range": [-4, 4],
	"add_augmentation": true,
	"balance_samples": false,
	"truth_cutoff_low": 0.15,
	"truth_cutoff_high": 0.85
    }
}
